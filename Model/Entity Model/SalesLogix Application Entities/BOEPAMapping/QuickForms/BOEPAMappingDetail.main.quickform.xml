          <MethodName>quickformload0</MethodName>
          <CSharpCodeSnippet>Sage.Platform.WebPortal.EntityPage page = Page as Sage.Platform.WebPortal.EntityPage;
IBOEPAMapping boePAMapping = BindingSource.Current as IBOEPAMapping; 
if (page != null)
{
	if(page.ModeId.ToUpper() == "INSERT")
	{
	  	if (Request.QueryString["copyboepamapping"] != null &amp;&amp; Request.QueryString["entityId"] != null &amp;&amp;
	         Request.QueryString["copyboepamapping"].Equals("true"))
		{
		    IBOEPAMapping copyBOEPAMapping = Sage.Platform.EntityFactory.GetById&lt;IBOEPAMapping&gt;(Request.QueryString["entityId"]);
		   	 boePAMapping.CloneBOEPAMapping(copyBOEPAMapping);
		}
	}
}

if (boePAMapping != null &amp;&amp; boePAMapping.IsCustom != null &amp;&amp; (bool) !boePAMapping.IsCustom)
{
	btnDelete.Visible = false;
}</CSharpCodeSnippet>
