          <MethodName>quickformload1</MethodName>
          <CSharpCodeSnippet>var mapping = BindingSource.Current as Sage.Entity.Interfaces.IBODMapping;
if (mapping != null &amp;&amp; mapping.IsCustom != null &amp;&amp; (bool) !mapping.IsCustom)
{
	//Disable Save and Delete toolbar buttons
	//btnSave.Visible = false;
	btnDelete.Visible = false;
	
	//Disable controls
	//Add here if we add new fields to the form
    lueBackOffice.Enabled = false;
    txtBODMappingName.Enabled = false;
    txtNoun.Enabled = false;
    cboEntityName.Attributes.Add("disabled", "");
    txtHandlerClass.Enabled = false;
    txtUniqueIDNode.Enabled = false;
    txtUniqueIDXPath.Enabled = false;
    txtPriority.Enabled = false;
    cboRecordOwner.Attributes.Add("disabled", "");
    //txtOutboundMsgTemplate.Enabled = false;
	//chkActive.Enabled = false;
	//chkOutboundOnCreate.Enabled = false;
	//chkOutboundOnUpdate.Enabled = false;
	//chkOutboundOnDelete.Enabled = false;
	chkGenOutTemplate.Enabled = false;
	QFSLXPickList.Enabled = false;
}

if (mapping != null &amp;&amp; mapping.SystemOfRecord == Saleslogix.Integration.BOE.Common.Constants.SystemOfRecord.CRM)
{
	txtHandlerClass.Enabled = false;
    txtUniqueIDNode.Enabled = false;
    txtUniqueIDXPath.Enabled = false;
	QFSLXPickList.Enabled = false;
	txtPriority.Enabled = false;
	cboRecordOwner.Attributes.Add("disabled", "");
}</CSharpCodeSnippet>
