      <Action typeName="Sage.Platform.QuickForms.ActionItems.CSharpSnippetActionItem, Sage.Platform.QuickForms">
        <CSharpSnippetActionItem>
          <MethodName>quickformload0</MethodName>
          <CSharpCodeSnippet>// get parent account
Sage.Entity.Interfaces.IAccount account = this.GetParentEntity() as Sage.Entity.Interfaces.IAccount;

// get current customer system
Sage.Entity.Interfaces.ICustomerSystem custSystem = this.BindingSource.Current as Sage.Entity.Interfaces.ICustomerSystem;

if (!custSystem.ValidDate.HasValue)
	custSystem.ValidDate = DateTime.Now;

if (custSystem.Account == null)
{
	custSystem.Account = account;
	foreach (Sage.Entity.Interfaces.IContact contact in account.Contacts)
	{
		if (contact.IsPrimary.HasValue &amp;&amp; contact.IsPrimary.Value == true)
		{
			custSystem.Contact = contact;
		}
	}
}</CSharpCodeSnippet>
        </CSharpSnippetActionItem>
